command=$HOME/.scripts/$BLOCK_NAME
separator_block_width=15
markup=pango

# [record]
# command=cat ~/.scripts/.recording
# interval=once
# signal=9

[i3mpd]
label=mpd
command=i3mpd
interval=once
align=left
signal=11

[i3weather]
command=i3weather
interval=3600

[i3mail]
label=mail📮
command=i3mail
interval=180
align=left
signal=12

[i3volume]
interval=once
command=i3volume
markup=pango
signal=10

